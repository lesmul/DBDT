<Window x:Class="DBDT.DXF.W_EDIT_DATA_DXF"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DBDT.DXF"
        mc:Ignorable="d"
        Title="Edytuj dane" Height="450" Width="930" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" ShowInTaskbar="False" Topmost="True" WindowStyle="ToolWindow" Loaded="Loaded_win" SizeToContent="Height" BorderThickness="0,0,0,0">
    <Grid Margin="0,0,0,0">
        <DataGrid x:Name="DG_DXF_OBJ" HorizontalAlignment="Left" Height="349" Margin="5,5,0,0" VerticalAlignment="Top" Width="805" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <Button x:Name="B_OK" Content="Zmień" HorizontalAlignment="Left" Height="26" Margin="824,4,0,0" VerticalAlignment="Top" Width="85" RenderTransformOrigin="0.211,0.862" Click="Click_OK"/>
        <TextBox x:Name="Canvas_TXT" HorizontalAlignment="Left" Height="43" Margin="5,380,0,11" TextWrapping="Wrap" Text="Canvas_TXT" Width="905" UndoLimit="1" ToolTip="Wygenerowane objekty" IsReadOnly="True" BorderThickness="1,1,1,1" SelectionOpacity="1"/>
        <Label Content="Canvas - objekty:" HorizontalAlignment="Left" Margin="5,354,0,0" VerticalAlignment="Top" Width="115" RenderTransformOrigin="0.478,0.618" Height="26"/>

    </Grid>
</Window>
